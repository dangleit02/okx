# get balance
  curl -X GET 'http://localhost:3030/balance?ccy=DOGS' \
    -H "Content-Type: application/json"    

# remove all trigger sell orders
curl -X POST "http://localhost:3030/sell-at-price-all-coins?\
testing=false&\
removeExistingSellOrders=true&\
  -H "Content-Type: application/json"


# autobuy for one coin
curl -X POST "http://localhost:3030/buy-at-price/OP?\
testing=false&\
removeExistingBuyOrders=true&\
