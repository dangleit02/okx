  -H "Content-Type: application/json"

# autobuy for all
curl -X POST "http://localhost:3030/buy-at-price-all-coins?\
testing=false&\
removeExistingBuyOrders=true&\
autobuy=true" \
  -H "Content-Type: application/json"

# sell when up for one coin (to take profit)
curl -X POST "http://localhost:3030/sell-at-price/BOME?\
testing=true&\
removeExistingSellOrders=true&\
addSellTakeProfit=true" \
-H "Content-Type: application/json"


# sell when up for all coinsSpotForTakeProfit (to take profit)
curl -X POST "http://localhost:3030/sell-at-price-all-coins?\
testing=false&\
removeExistingSellOrders=true&\
addSellTakeProfit=true" \
-H "Content-Type: application/json"

# sell when down for one coin (to take profit but when down like stop loss)
curl -X POST "http://localhost:3030/sell-at-price/AVAX?\
testing=false&\
removeExistingSellOrders=true&\
addSellTakeProfit=true&\
onlyForDown=true&\
justOneOrder=true" \
  -H "Content-Type: application/json"

# sell when down for all coinsSpotForTakeProfit (to take profit but when down like stop loss)
curl -X POST "http://localhost:3030/sell-at-price-all-coins?\
testing=false&\
removeExistingSellOrders=true&\
addSellTakeProfit=true&\
onlyForDown=true&\
justOneOrder=true" \
  -H "Content-Type: application/json"

